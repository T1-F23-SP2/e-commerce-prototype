<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="1000.0" prefWidth="180.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ecommerceprototype.cms.SortFilter">
   <children>
      <HBox prefHeight="40.0" prefWidth="180.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="filterImage_ImageView" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="filterTitle_Label" prefHeight="40.0" prefWidth="140.0" text="Filter">
               <padding>
                  <Insets left="5.0" />
               </padding>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ScrollPane fx:id="filterScroll_ScrollPane" fitToWidth="true" prefHeight="960.0" prefWidth="180.0" AnchorPane.topAnchor="40.0">
         <content>
            <TitledPane animated="false" text="Price">
               <content>
                  <VBox prefHeight="0.0" prefWidth="176.0">
                     <children>
                        <HBox prefHeight="20.0" prefWidth="180.0">
                           <children>
                              <Label prefHeight="20.0" prefWidth="60.0" text="Label" />
                              <Label prefHeight="20.0" prefWidth="40.0" text="Label" textAlignment="RIGHT" />
                           </children>
                        </HBox>
                        <Slider />
                     </children>
                  </VBox>
               </content>
            </TitledPane>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
