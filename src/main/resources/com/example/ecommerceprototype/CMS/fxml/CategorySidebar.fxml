<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1000.0" prefWidth="180.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER">
   <Label alignment="CENTER" prefHeight="40.0" prefWidth="180.0" text="Category">
         <font>
            <Font size="24.0" />
         </font>
   </Label>
   <VBox fx:id="categoryList_VBox" prefWidth="180.0" spacing="10">
      <padding>
         <Insets bottom="25"/>
      </padding>
   </VBox>
   <Label alignment="CENTER" prefHeight="40.0" prefWidth="180.0" text="Articles">
      <font>
         <Font size="24.0" />
      </font>
   </Label>
   <Button fx:id="articlesButton_Button" text="Go To Articles" prefWidth="180" prefHeight="50">
      <font>
         <Font size="16"/>
      </font>
   </Button>
</VBox>
